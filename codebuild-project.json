{
    "name": "aws-codebuild-python-lambda",
    "source": {
        "type": "GITHUB",
        "location": "https://github.com/PraveenKumar-Kasoju/aws-codebuild-python-lambda.git",
        "buildspec": "buildspec.yml"
    },
    "artifacts": {
        "type": "NO_ARTIFACTS"
    },
    "environment": {
        "type": "LINUX_CONTAINER",
        "image": "aws/codebuild/standard:5.0",
        "computeType": "BUILD_GENERAL1_SMALL",
        "privilegedMode": true
    },
    "serviceRole": "arn:aws:iam::YOUR_ACCOUNT_ID:role/YourCodeBuildServiceRole"
}

